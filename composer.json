{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "4.2.*",
    "katzgrau/klogger": "1.0.*",
    "phpunit/phpunit": "4.0.*",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit-selenium": ">=1.2",
    "facebook/webdriver": "dev-master",
    "google/apiclient": "1.0.*@beta",
    "tymon/jwt-auth": "0.4.*",
    "jenssegers/agent": "^2.1",
    "bendani/php-common/login-service": "1.0.*",
    "bendani/php-common/google-image-service": "dev-master"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/repository",
      "app/domain",
      "app/service",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/garagepoort/php-common-loginservice.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/garagepoort/php-common-googleimageservice.git"
    }
  ]
}
