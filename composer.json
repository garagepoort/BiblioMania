{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "4.2.*",
    "katzgrau/klogger": "1.0.*",
    "phpunit/phpunit": "4.0.*",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit-selenium": ">=1.2",
    "facebook/webdriver": "dev-master",
    "google/apiclient": "1.0.*@beta",
    "tymon/jwt-auth": "0.4.*",
    "jenssegers/agent": "^2.1",
    "netresearch/jsonmapper": "^0.10.0",
    "webpatser/laravel-uuid": "2.*",
    "doctrine/dbal": "~2.3",
    "elasticsearch/elasticsearch": "~2.0@beta",
    "monolog/monolog": "~1.0",

    "bendani/php-common/login-service": "3.0.*",
    "bendani/php-common/google-image-service": "1.0.*",
    "bendani/php-common/filter-service": "5.1.*",
    "bendani/php-common/uiframework": "1.3.*",
    "bendani/php-common/utils": "2.0.*"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/repository",
      "app/domain",
      "app/service",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "php artisan asset:publish bendani/php-common/filter-service",
      "php artisan asset:publish bendani/php-common/login-service",
      "php artisan asset:publish bendani/php-common/uiframework"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/garagepoort/php-common-loginservice.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/garagepoort/php-common-googleimageservice.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/garagepoort/php-common-filterservice.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/garagepoort/php-common-utils.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/garagepoort/php-common-uiframework.git"
    }
  ]
}
