{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",

        "katzgrau/klogger": "1.0.*",
        "phpunit/phpunit-selenium": ">=1.2",
        "facebook/webdriver": "dev-master",
        "tymon/jwt-auth": "0.5.*",
        "jenssegers/agent": "^2.1",
        "netresearch/jsonmapper": "^0.10.0",
        "webpatser/laravel-uuid": "2.*",
        "doctrine/dbal": "~2.3",
        "elasticsearch/elasticsearch": "~2.0@beta",
        "monolog/monolog": "~1.0",
        "laravelcollective/annotations": "~5.0",

        "bendani/php-common/login-service": "^v4.0.0",
        "bendani/php-common/uiframework": "^v2.1.0",
        "bendani/php-common/filter-service": "^v6.0.0",
        "bendani/php-common/utils": "^v3.0.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "classmap": [
            "database",
            "app/Http/Controllers",
            "app/models",
            "app/service",
            "app/repository",
            "tests",
            "vendor/bendani"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize",
            "php artisan vendor:publish"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/garagepoort/php-common-loginservice.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/garagepoort/php-common-filterservice.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/garagepoort/php-common-utils.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/garagepoort/php-common-uiframework.git"
        }
    ]
}
